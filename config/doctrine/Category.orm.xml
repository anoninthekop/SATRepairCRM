<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="App\Repository\CategoryRepository" name="App\Entity\Category" table="category">
    <id name="id" type="uuid" column="id"/>
    <field name="name" type="text" column="name"/>
    <field name="description" type="text" column="description" nullable="true"/>
    <field name="modified" type="datetime" column="modified" nullable="true"/>
    <field name="created" type="datetime_immutable" column="created"/>
  </entity>
</doctrine-mapping>
