<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="App\Repository\CustomerRepository" name="App\Entity\Customer" table="customer">
    <id name="id" type="uuid" column="id"/>
    <field name="fullname" column="fullname" length="70" precision="0" scale="0"/>
    <field name="address" column="address" length="70" precision="0" scale="0" nullable="true"/>
    <field name="city" column="city" length="50" precision="0" scale="0" nullable="true"/>
    <field name="email" column="email" length="70" precision="0" scale="0" nullable="true"/>
    <field name="zipCode" type="integer" column="zip_code" precision="0" scale="0" nullable="true"/>
    <field name="phone" type="integer" column="phone" precision="0" scale="0"/>
    <field name="modified" type="datetime" column="modified" precision="0" scale="0" nullable="true"/>
    <field name="created" type="datetime_immutable" column="created" precision="0" scale="0"/>
  </entity>
</doctrine-mapping>
