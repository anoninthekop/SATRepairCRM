<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>SATRepairCRM Tracker</title>
    <link rel="shortcut icon" href="{{ asset('build/images/favicon.ico') }}" type="image/x-icon">

    {% block stylesheets %}
        {{ encore_entry_link_tags('tracker') }}
    {% endblock %}
</head>
<body>
    <main class="tracker__block">
        <img src="{{ asset('build/images/logo-front-slogan-rec.png') }}" alt="SATRepairCRM Logo">

        {{ include('tracker/_form.html.twig') }}

        <div id="content__block"></div>
    </main>
    <footer class="footer__block">
        <a href="{{ path('privacy') }}" target="_blank">Privacy Policy</a> |
        <a href="{{ path('terms') }}" target="_blank">Terms & Conditions</a>
    </footer>

    {% block javascripts %}
        {{ encore_entry_script_tags('tracker') }}
    {% endblock %}
</body>
</html>
